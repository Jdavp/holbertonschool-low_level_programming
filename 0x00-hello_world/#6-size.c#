#include <stdio.h>

a**
 *main - Entry point
 *
 *Return: Always 0 (Success)
 */
int main (void)
{
	printf("Size of a char: %C byte(s)");
	printf("Size of an int: %d byte(s)");
	printf("Size of a long int: %ld byte(s)");
	printf("Size of a long long int: %lld byte(s)");
	printf("Size of a float: %f byte(s)");
			return (0);
}
